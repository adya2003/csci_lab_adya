#include <stdio.h>
#include <string.h>

#define NUM_TEAMS 3
#define SQUAD_SIZE 5

struct Player {
    char name[50];
    int kitNumber;
    int age;
};

struct Team {
    char name[50];
    struct Player players[SQUAD_SIZE];
    int activeSize;
};

int main() {
    struct Team teams[NUM_TEAMS];
    int teamCount = 0;

    // Example of enrolling a team
    strcpy(teams[teamCount].name, "Team A");
    teams[teamCount].activeSize = 0;
    teamCount++;

    // Example of adding a player
    strcpy(teams[0].players[0].name, "Player 1");
    teams[0].players[0].kitNumber = 10;
    teams[0].players[0].age = 20;
    teams[0].activeSize++;

    // Print details
    printf("Team: %s\n", teams[0].name);
    printf("Player: %s, Kit Number: %d, Age: %d\n",
           teams[0].players[0].name,
           teams[0].players[0].kitNumber,
           teams[0].players[0].age);

    return 0;
}
